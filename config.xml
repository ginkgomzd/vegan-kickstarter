<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="1709080001" id="org.pcrm.VeganKickstartES" version="1.0.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Vegetariano en 21 días</name>
    <description>
        21 días de recetas fáciles con fotos bonitas.
    </description>
    <author email="dev@ginkgostreet.com" href="http://www.ginkgostreet.com">
        Ginkgo Street Labs
    </author>
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="SplashScreen" value="screen" />
    <preference name="AutoHideSplashScreen" value="false" />
    <preference name="StatusBarBackgroundColor" value="#B2D235" />
    <hook src="scripts/build_ember_app.sh" type="before_build" />
    <platform name="android">
        <content src="index.html" />
        <resource-file src="GTM-5MF2C8B.json" target="assets/containers/GTM-5MF2C8B.json" />
        <preference name="android-minSdkVersion" value="15" />
        <preference name="android-targetSdkVersion" value="22" />
        <allow-intent href="market:*" />
        <framework src="com.facebook.android:facebook-android-sdk:4.8.0" />
        <preference name="SplashMaintainAspectRatio" value="true" />
        <preference name="AndroidPersistentFileLocation" value="Internal" />
        <icon density="ldpi" src="res/icons/icon-ldpi.png" />
        <icon density="mdpi" src="res/icons/icon-mdpi.png" />
        <icon density="hdpi" src="res/icons/icon-hdpi.png" />
        <icon density="xhdpi" src="res/icons/icon-xhdpi.png" />
        <icon density="xxhdpi" src="res/icons/icon-xxhdpi.png" />
        <icon density="xxxhdpi" src="res/icons/icon-xxxhdpi.png" />
        <splash density="port-hdpi" src="res/splash/android-h.png" />
        <splash density="port-ldpi" src="res/splash/android-l.png" />
        <splash density="port-mdpi" src="res/splash/android-m.png" />
        <splash density="port-xhdpi" src="res/splash/android-x.png" />
        <splash density="port-xxhdpi" src="res/splash/android-xx.png" />
        <splash density="port-xxxhdpi" src="res/splash/android-splash.png" />
    </platform>
    <platform name="ios">
        <content src="index.html" />
        <preference name="BackupWebStorage" value="none" />
        <preference name="StatusBarOverlaysWebView" value="false" />
        <preference name="iosPersistentFileLocation" value="Library" />
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <icon height="180" src="res/icons/icon-60@3x.png" width="180" />
        <icon height="60" src="res/icons/icon-60.png" width="60" />
        <icon height="120" src="res/icons/icon-60@2x.png" width="120" />
        <icon height="76" src="res/icons/icon-76.png" width="76" />
        <icon height="152" src="res/icons/icon-76@2x.png" width="152" />
        <icon height="40" src="res/icons/icon-40.png" width="40" />
        <icon height="80" src="res/icons/icon-40@2x.png" width="80" />
        <icon height="57" src="res/icons/icon-@1x.png" width="57" />
        <icon height="114" src="res/icons/icon-@2x.png" width="114" />
        <icon height="72" src="res/icons/icon-72.png" width="72" />
        <icon height="144" src="res/icons/icon-72@2x.png" width="144" />
        <icon height="167" src="res/icons/icon-83.5@2x.png" width="167" />
        <icon height="29" src="res/icons/icon-small.png" width="29" />
        <icon height="58" src="res/icons/icon-small@2x.png" width="58" />
        <icon height="87" src="res/icons/icon-small@3x.png" width="87" />
        <icon height="50" src="res/icons/icon-50.png" width="50" />
        <icon height="100" src="res/icons/icon-50@2x.png" width="100" />
        <icon height="1024" src="res/icons/icon-1024.png" width="1024" />
        <splash height="480" src="res/splash/iphone/Default.png" width="320" />
        <splash height="960" src="res/splash/iphone/Default@2x.png" width="640" />
        <splash height="1136" src="res/splash/iphone/Default-568h@2x.png" width="640" />
        <splash height="1334" src="res/splash/iphone/Default-667h@2x.png" width="750" />
        <splash height="2208" src="res/splash/iphone/Default-736@2x.png" width="1242" />
        <splash height="1024" src="res/splash/ipad/Default-Portrait.png" width="768" />
        <splash height="2048" src="res/splash/ipad/Default-Portrait@2x.png" width="1536" />
    </platform>
    <engine name="android" spec="^6.2.3" />
    <engine name="ios" spec="^4.5.4" />
    <plugin name="cordova-plugin-facebook4" spec="git+https://github.com/jeduan/cordova-plugin-facebook4.git">
        <variable name="APP_ID" value="1661929367400739" />
        <variable name="APP_NAME" value="21DayVeganKickstart" />
    </plugin>
    <plugin name="cordova-plugin-file-transfer" spec="^1.7.0" />
    <plugin name="cordova-plugin-inappbrowser" spec="^1.7.2" />
    <plugin name="cordova-plugin-spinner-dialog" spec="git+https://github.com/Paldom/SpinnerDialog.git" />
    <plugin name="cordova-plugin-splashscreen" spec="^3.2.2" />
    <plugin name="cordova-plugin-statusbar" spec="^2.3.0" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
    <plugin name="cordova-plugin-x-socialsharing" spec="^5.2.1" />
    <plugin name="cordova-plugin-firebase" spec="^0.1.25" />
</widget>
