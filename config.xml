<?xml version='1.0' encoding='utf-8'?>
<widget id="org.pcrm.VeganKickstartES" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>VeganKickstart</name>
    <description>
        A 21 Day Vegan Kickstarter! Spanish
    </description>
    <author email="dev@ginkgostreet.com" href="http://www.ginkgostreet.com">
        Ginkgo Street Labs
    </author>
    <content src="http://localhost:4200" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="StatusBarBackgroundColor" value="#B2D235" />
    <hook src="scripts/build_ember_app.sh" type="before_prepare" />
    <platform name="android">
        <preference name="android-minSdkVersion" value="15" />
        <preference name="android-targetSdkVersion" value="22" />
        <allow-intent href="market:*" />
        <framework src="com.facebook.android:facebook-android-sdk:4.8.0" />
        <preference name="AndroidPersistentFileLocation" value="Compatibility" />
    </platform>
    <platform name="ios">
        <preference name="BackupWebStorage" value="none" />
        <preference name="StatusBarOverlaysWebView" value="false" />
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="cordova-plugin-facebook4" spec="https://github.com/jeduan/cordova-plugin-facebook4">
        <variable name="APP_ID" value="1661929367400739" />
        <variable name="APP_NAME" value="21DayVeganKickstart" />
    </plugin>
    <plugin name="phonegap-plugin-push" spec="^1.6.0">
        <variable name="SENDER_ID" value="155595325949" />
    </plugin>
    <plugin name="cordova-labs-wkwebview-engine-localhost" spec="https://github.com/apache/cordova-plugins.git#master:wkwebview-engine-localhost" />
</widget>
